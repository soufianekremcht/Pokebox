<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="style_pokemon_detail_text">


    </style>

    <style name="style_pokemon_detail_card">


    </style>

    <style name="style_pokemon_detail_layout">
    </style>

    <style name="style_pokemon_detail_progress_text" >
        <item name="android:layout_margin">@dimen/spacing_small</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textSize">@dimen/font_normal</item>
        <item name="android:textStyle">bold</item>

    </style>

    <style name="style_pokemon_detail_progress_bar">
        <item name="android:layout_margin">@dimen/spacing_small</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="progressView_colorBackground">@color/white</item>
        <item name="progressView_padding">1dp</item>
        <item name="progressView_radius">20dp</item>
        <item name="progressView_min">0</item>
        <item name="progressView_labelColorInner">@color/white</item>
        <item name="progressView_labelColorOuter">@color/black</item>
        <item name="progressView_labelSize">13dp</item>
        <item name="progressView_labelSpace">10dp</item>
        <item name="progressView_labelTypeface">bold</item>
        <item name="progressView_autoAnimate">true</item>
    </style>

    <style name="style_pokemon_detail_stat_layout">
        <item name="android:height">30dp</item>
    </style>
</resources>